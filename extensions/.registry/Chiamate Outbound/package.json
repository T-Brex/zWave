{
  "name": "directus-extension-conversazioni",
  "version": "1.0.0",
  "description": "Modulo Conversazioni per Directus",
  "type": "module",
  "keywords": [
    "directus",
    "directus-extension",
    "directus-custom-module"
  ],
  "main": "dist/app.js",
  "files": [
    "dist"
  ],
  "directus:extension": {
    "type": "bundle",
    "path": {
      "app": "dist/app.js",
      "api": "dist/api.js"
    },
    "entries": [
      {
        "type": "module",
        "name": "chiamate-outbound",
        "source": "src/index.js"
      },
      {
        "type": "hook",
        "name": "chiamate-outbound-lead-webhook",
        "source": "src/hook/index.js"
      }
    ],
    "host": "^10.0.0"
  },
  "scripts": {
    "build": "directus-extension build",
    "dev": "directus-extension build -w --no-minify"
  },
  "devDependencies": {
    "@directus/extensions-sdk": "10.1.0",
    "vue": "^3.2.47"
  },
  "peerDependencies": {
    "vue": "^3.2.47"
  }
}
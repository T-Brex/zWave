{
	"name": "Interfaccia",
	"description": "Nasconde le sezioni Collections, Elenco Utenti, Documentazione, Libreria File e sidebar destra per utenti non admin",
	"icon": "extension",
	"version": "1.0.0",
	"keywords": [
		"directus",
		"directus-extension",
		"directus-extension-hook",
		"hide-collections",
		"non-admin"
	],
	"type": "module",
	"files": [
		"dist"
	],
	"directus:extension": {
		"type": "hook",
		"path": "dist/index.js",
		"source": "src/index.js",
		"host": "^10.10.0"
	},
	"scripts": {
		"build": "directus-extension build",
		"dev": "directus-extension build -w --no-minify",
		"link": "directus-extension link",
		"validate": "directus-extension validate"
	},
	"devDependencies": {
		"@directus/extensions-sdk": "17.0.5",
		"vue": "^3.5.27"
	}
}
